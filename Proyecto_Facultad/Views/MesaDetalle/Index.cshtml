@model IEnumerable<dynamic>

<div class="container mt-4">
<h1 class="mb-4">Mesas</h1>

<div class="table-responsive mb-4">
        <table class="table table-striped table-bordered">
    
    <thead class="table-dark">
        <tr>
            <th scope="col">Id Mesa</th>
            <th scope="col">Sede</th>
            <th scope="col">Jornada</th>
            <th scope="col">Fecha Inicio</th>
            <th scope="col">Fecha Fin</th>
            <th scope="col">Estado</th>
            <th scope="col">Detalles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mesa in Model)
        {
            <tr>
                <td>@mesa.IdMesa</td>
                <td>@mesa.SedeDescripcion</td>
                <td>@mesa.JornadaDescripcion</td>
                <td>@mesa.FechaInicio</td>
                <td>@mesa.FechaFin</td>
                <td>@mesa.EstadoMesa</td>
                <td>
                    <a href="@Url.Action("Detalle", new { id = mesa.IdMesa })">Ver Detalles</a>
                </td>
            </tr>
        }
    </tbody>
</table>
</div>
</div>